{
  "name": "pntsurveyapp",
  "version": "1.0.0",
  "description": "P&T Survey App",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^5.8.2",
    "@sap/cds-odata-v2-adapter-proxy": "^1.8.9",
    "@sap/hana-client": "^2.11.20",
    "@sap/xsenv": "^3.1.1",
    "@sap/xssec": "^3.2.13",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "hdb": "^0.18.3",
    "nodemailer": "^6.7.3",
    "passport": "^0.4.1",
    "sap-cf-mailer": "0.0.5"
  },
  "devDependencies": {
    "@sap/hdi-deploy": "4.2.0",
    "@sap/ux-specification": "^1.96.4",
    "sqlite3": "^5.0.2"
  },
  "scripts": {
    "start": "cds run",
    "undeploy": "cf undeploy pntsurveyapp --delete-services --delete-service-keys"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2020": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "globals": {
      "SELECT": true,
      "INSERT": true,
      "UPDATE": true,
      "DELETE": true,
      "CREATE": true,
      "DROP": true,
      "CDL": true,
      "CQL": true,
      "CXL": true,
      "cds": true
    },
    "rules": {
      "no-console": "off",
      "require-atomic-updates": "off"
    }
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sql"
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    }
  }
}
